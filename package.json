    "@types/auth0-js": "9.10.6",
    "@types/lodash": "^4.14.136",
    "cross-env": "^6.0.3",
    "publish-to-git": "github:tfso/publish-to-git",
    "rollup": "1.27.14",
    "tslib": "^1.10.0",
    "typescript": "3.7.2"
  },
  "files": [
    "dist",
    "src"
  ]
