{
  "name": "js-tfso-auth",
  "version": "2.0.14",
  "private": true,
  "main": "dist/index.js",
  "globalModule": "dist/TfsoAuth.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "npm run build:clean && npm run build:module && npm run build:browser",
